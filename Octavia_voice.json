# --- Option A: Process ONLY the structured blocks (Scenario/Desc/Aspects) ---
processed_data_structured = []
i = 0
while i < len(data):
    current_item = data[i]
    current_text = current_item.get('text', '')

    if current_text.startswith('Scenario:'):
        scenario_block = {'context': None, 'quote': None, 'description': None, 'aspects': []}
        block_items_consumed = 0
        try:
            lines = current_text.split('\n')
            if len(lines) > 0: scenario_block['context'] = lines[0].replace('Scenario:', '').strip()
            if len(lines) > 1: scenario_block['quote'] = lines[1].replace('Octavia Opulence says:', '').strip()
            block_items_consumed += 1

            if i + 1 < len(data) and data[i + 1].get('text', '').startswith('Describe Octavia Opulence'):
                lines = data[i + 1]['text'].split('\n')
                if len(lines) > 1: scenario_block['description'] = lines[1].replace('Description:', '').strip()
                block_items_consumed += 1
            else:
                 # print(f"Warning: Expected Description at index {i+1}, found different structure. Skipping block.")
                 i += 1; continue # Skip scenario block if no description follows

            j = i + block_items_consumed
            while j < len(data) and data[j].get('text', '').startswith('What aspect'):
                lines = data[j]['text'].split('\n')
                if len(lines) > 1: scenario_block['aspects'].append(lines[1].replace('Aspect:', '').strip())
                block_items_consumed += 1
                j += 1

            if scenario_block['context'] and scenario_block['description']: # Check if essential parts were found
                 processed_data_structured.append(scenario_block)
            else:
                 print(f"Warning: Incomplete structured block found starting at index {i}. Skipping.")

            i += block_items_consumed
        except Exception as e:
            print(f"Error processing potential block starting at index {i}: {e}")
            i += 1
    else:
        i += 1 # Move to next item if not a scenario start

print("--- Processed Structured Scenario Data ---")
print(json.dumps(processed_data_structured, indent=2))
print(f"\nProcessed {len(processed_data_structured)} structured blocks.")


# --- Option B: Process ALL data into a consistent format (e.g., instruction/output) ---
processed_data_instruction_output = []
for item in data:
    text_content = item.get('text')
    if not text_content: continue # Skip empty items

    # Attempt to parse different known structures
    if text_content.startswith("Scenario:"):
        lines = text_content.split('\n')
        if len(lines) > 1:
            instruction = lines[0].strip() # Use the scenario line as instruction
            output = lines[1].replace('Octavia Opulence says:', '').strip()
            processed_data_instruction_output.append({"instruction": instruction, "output": output})
    elif text_content.startswith("Describe Octavia Opulence"):
         # Maybe skip descriptions or rephrase as Q/A
         pass
    elif text_content.startswith("What aspect"):
         # Maybe skip aspects or rephrase as Q/A
         pass
    elif text_content.startswith("Octavia Opulence's perspective"):
         # Use the whole text as output, maybe create a generic instruction
         instruction = "Share Octavia Opulence's perspective on authenticity and luxury."
         output = text_content.replace("Octavia Opulence's perspective on authenticity and luxury:", "").strip()
         processed_data_instruction_output.append({"instruction": instruction, "output": output})
    elif text_content.startswith("Octavia Opulence reading"):
         instruction = "Read someone for filth in Octavia Opulence's style."
         output = text_content.replace("Octavia Opulence reading someone for filth:", "").strip()
         processed_data_instruction_output.append({"instruction": instruction, "output": output})
    elif text_content.startswith("Question:"):
         lines = text_content.split('\n')
         if len(lines) > 1:
             instruction = lines[0].replace("Question:", "").strip()
             output = lines[1].replace("Octavia Opulence:", "").strip()
             processed_data_instruction_output.append({"instruction": instruction, "output": output})
    # Add handling for the instruction/output format if it exists directly
    elif 'instruction' in item and 'output' in item:
         processed_data_instruction_output.append({"instruction": item['instruction'], "output": item['output']})
    # Add handling for the quote/tags/context format
    elif 'quote' in item and 'context' in item:
         instruction = f"Provide a quote from Octavia Opulence about: {item['context']}"
         output = item['quote']
         processed_data_instruction_output.append({"instruction": instruction, "output": output})


print("\n--- Processed Instruction/Output Data ---")
print(json.dumps(processed_data_instruction_output[:5], indent=2)) # Print first 5 examples
print(f"\nProcessed {len(processed_data_instruction_output)} items into instruction/output format.")

# --- Save the desired processed data to a file ---
# Example saving the instruction/output format
output_filename = 'processed_octavia_data.jsonl'
with open(output_filename, 'w') as f:
    for item in processed_data_instruction_output:
        f.write(json.dumps(item) + '\n')
print(f"\nSaved processed instruction/output data to {output_filename}")

